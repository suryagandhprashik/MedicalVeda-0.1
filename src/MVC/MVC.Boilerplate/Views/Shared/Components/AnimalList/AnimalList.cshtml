@model IEnumerable<MVC.Boilerplate.Models.Lazy.Animal>

    <table id="table_id" class="table table-striped table-bordered">
                <thead>
                    <tr>
                        <th>Id</th>
                        <th>Name</th>
                        <th>Type</th>
                    </tr>
                </thead>
                <tbody>
        @foreach(var animal in Model)
        {
            <tr>
                <td>@animal.Id</td>
                <td>@animal.Name</td>
                <td>@animal.Type</td>
            </tr>
        }
                </tbody>
            </table>