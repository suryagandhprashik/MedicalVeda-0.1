@model MVC.Boilerplate.Models.Order.Orders
@{
    ViewData["Title"] = "ORDER LIST";
}


<div>
    <nav aria-label="breadcrumb">
        <ol class="breadcrumb">
        <li class="breadcrumb-item"><a href="@Url.Action("Index", "Home")">Home</a></li>
        <li class="breadcrumb-item active"><a>Order List</a></li>
        </ol>
    </nav>
</div>

<h3>@ViewData["Title"]</h3>
<br />

<div class="row">
    <div class="col-md-12">
        <table id="test-registers" class="table table-striped table-bordered">
            <thead>
                <tr>
                    <th>Order Id</th>
                    <th>Total Price</th>
                    <th>Order Placed Date</th>
                </tr>
            </thead>
        </table>
    </div>
</div>



@section Scripts{
    <script src="~/scripts/order.js" asp-append-version="true"></script>
    <script src=" https://momentjs.com/downloads/moment.min.js"></script>  
}